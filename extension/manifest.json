{
    "manifest_version": 3,
    "name": "TopCharts",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
        "default_popup": "Chart.html",
        "default_icon": "chart.jpg"
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["popup.js"]
        }
    ]
}
