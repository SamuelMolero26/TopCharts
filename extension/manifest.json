{
    "manifest_version": 3,
    "name": "TopCharts",
    "description": "Base Level Extension",
    "version": "1.0",
    "background": {
        "service_worker": "background.js"

    },
    "action": {
      "default_popup": "Chart.html",
      "default_icon": "chart.jpg"
    },
    "host_permissions": [
        "http://localhost:5000/scripts"
    ]
}